@import "./common/wepy-swipe-delete.wxss";
.list_box {
  height: 150rpx;
}
.address .list_box:last-child {
  border-bottom: 1px solid #efefef;
}
.address .address_list {
  display: flex;
  justify-content: space-between;
  height: 150rpx;
  align-items: center;
  padding: 0rpx 0px 0px 35rpx;
  border-top: 1px solid #efefef;
}
.address .address_list .title {
  color: #000;
}
.address .address_list .arrow {
  height: 150rpx;
  line-height: 150rpx;
  width: 150rpx;
  text-align: center;
  color: #9a9a9a;
}
.address .address_list .user_info {
  color: #1a1a1a;
}
.address .address_list .active_address {
  margin-top: 20rpx;
}
.address .address_list .defult {
  color: #ea4a3a;
}
.add_wrap {
  display: block;
  width: 95%;
  left: 0;
  right: 0;
  position: absolute;
  bottom: 20rpx;
  margin: 0 auto;
}
